main.main
    .main-title-block
        h1.main__title Hello Evano üëãüèº,
        img.mobile-menu__open(src="images/mobile-menu.svg", alt="mobile-menu")
    section.main__content
        .main__content-top-block
            .main__content-top-left
                h2.main__content-title All Customers
                p.main__content-status Active Members
            .main__content-top-right
                search.search
                    .search__input-block
                        label.search__label-hidden(for="search") Search
                        input.search__input(type="text", id="search" placeholder="Search")
                        img.search__img(src="images/search-1.svg", alt="search")
        .table-underline
        table(id="customers")
            thead
                tr
                    th(scope="col") Customer Name
                    th(scope="col") Company
                    th(scope="col") Phone Number
                    th(scope="col") Email
                    th(scope="col") Country
                    th(scope="col") Status
                tr
            thead
            tbody
                each val in customers
                    tr
                        td(data-label="Customer Name") #{val.customersName}
                        td(data-label="Company") #{val.company}
                        td(data-label="Phone Number") #{val.phoneNumber}
                        td(data-label="Email") #{val.email}
                        td(data-label="Country") #{val.country}
                        td(data-label="Status")
                            if val.status === 'Active'
                                button.table-btn.td-active #{val.status}
                            else if val.status === 'Inactive'
                                button.table-btn.td-inactive #{val.status}
                    tr
        .main__content-bottom-block
            p.bottom-block-text Showing data 1 to 8 of  256K entries
            ul.bottom-block-list
                li.bottom-block-item
                    a.bottom-block-link
                        img.bottom-block-img(src="images/arrow-left.svg", alt="arrow")
                li.bottom-block-item
                    a.bottom-block-link.pagination-active 1
                li.bottom-block-item
                    a.bottom-block-link 2
                li.bottom-block-item
                    a.bottom-block-link 3
                li.bottom-block-item
                    a.bottom-block-link 4
                li.bottom-block-item
                    a.empty-link &hellip;
                li.bottom-block-item
                    a.bottom-block-link 40
                li.bottom-block-item
                    a.bottom-block-link
                        img.bottom-block-img(src="images/arrow-right.svg", alt="arrow")